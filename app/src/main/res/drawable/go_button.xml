<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:width="170dp" android:height="170dp">
      <shape android:shape="oval">
          <solid android:color="#49912f"/>
          <stroke
              android:color="#111111"/>
      </shape>
    </item>

    <item android:height="70dp" android:width="70dp"
        android:gravity="center">

        <shape android:shape="oval">
            <solid android:color="#dce233"/>
            <stroke
                android:color="#111111"
                android:width="3dp"/>
        </shape>
    </item>
</layer-list>