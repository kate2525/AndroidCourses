<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kate.main.MainActivity">

    <Button
        android:id="@+id/home1Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="home1"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/home2Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="home2"
        android:layout_below="@+id/home1Button"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/lesson2Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="myView"
        android:layout_marginEnd="11dp"
        android:layout_above="@+id/home2Button"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/home3Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="home3"
        android:layout_below="@+id/home2Button"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/lesson3Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lesson3"
        android:layout_alignBaseline="@+id/home2Button"
        android:layout_alignBottom="@+id/home2Button"
        android:layout_alignStart="@+id/lesson2Button" />

    <Button
        android:id="@+id/home4Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="myView"
        android:layout_below="@+id/home3Button"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/lesson4Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lesson4"
        android:layout_below="@+id/lesson3Button"
        android:layout_alignStart="@+id/lesson3Button" />

    <Button
        android:id="@+id/home5Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="home5"
        android:layout_below="@+id/home4Button"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/lesson5Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/lesson4Button"
        android:layout_below="@+id/lesson4Button"
        android:text="recycle" />

    <Button
        android:id="@+id/home6Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="home6"
        android:layout_below="@+id/home5Button"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/lesson6Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lesson6"
        android:layout_below="@+id/lesson5Button"
        android:layout_alignStart="@+id/lesson5Button" />

    <Button
        android:id="@+id/lesson7Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lesson7"
        android:layout_below="@+id/lesson6Button"
        android:layout_alignStart="@+id/lesson6Button" />

    <Button
        android:id="@+id/lesson8Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lesson8"
        android:layout_below="@+id/lesson7Button"
        android:layout_alignStart="@+id/lesson7Button" />
    <Button
        android:id="@+id/home10Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="rest_h10"
        android:layout_below="@+id/home9Button"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/lesson12Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="lesson12"
        android:layout_below="@+id/lesson8Button"
        android:layout_alignStart="@+id/lesson8Button" />

    <Button
        android:id="@+id/home9Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/home6Button"
        android:text="domain@pref_h9" />

    <Button
        android:id="@+id/lesson13Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/lesson8Button"
        android:text="fragment"
        android:layout_below="@+id/lesson12Button" />

    <Button
        android:id="@+id/lesson14Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SharedPref"
        android:layout_below="@+id/lesson13Button"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/home14Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Spinner_h14"
        android:layout_below="@+id/home10Button"
        android:layout_alignParentStart="true" />


</RelativeLayout>
